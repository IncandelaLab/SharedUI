<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
	<TYPE>
		<EXTENSION_TABLE_NAME>HGC_MOD_ASMBLY</EXTENSION_TABLE_NAME>
		<NAME>HGC Eight Inch Module Assembly</NAME>
	</TYPE>
	<RUN>
		<RUN_NAME>Module Assembly {{ run_name_time_stamp }}</RUN_NAME>
		<!-- <RUN_NUMBER>{{ step_pcb_num }}</RUN_NUMBER> -->
		<RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
		<RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP>
		<INITIATED_BY_USER>{{ initiated_by_user }}</INITIATED_BY_USER>
		<LOCATION>{{ location }}</LOCATION>
		<COMMENT_DESCRIPTION>Module assembly</COMMENT_DESCRIPTION>
	</RUN>
</HEADER>
	<DATA_SET>
		<COMMENT_DESCRIPTION>Module Assembly</COMMENT_DESCRIPTION>
		<VERSION>1</VERSION>
		<PART>
			<KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
			<SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
		</PART>
		<DATA>
			<ASMBL_TRAY_NAME>{{ asmbl_tray_name }}</ASMBL_TRAY_NAME>
			<PRTO_SER_NUM>{{ protomodule }}</PRTO_SER_NUM>
			<PRTO_ASM_ROW>{{ asm_row }}</PRTO_ASM_ROW>
			<PRTO_ASM_COL>{{ asm_col }}</PRTO_ASM_COL>
			<COMP_TRAY_NAME>{{ comp_tray_name }}</COMP_TRAY_NAME>
			<PCB_SER_NUM>{{ pcb }}</PCB_SER_NUM>
			<PCB_THKNES_MM>{{ pcb_thickness }}</PCB_THKNES_MM>
			<PCB_CMP_ROW>{{ asm_row }}</PCB_CMP_ROW>
			<PCB_CMP_COL>{{ asm_col }}</PCB_CMP_COL>
			<PCB_TOOL_NAME>{{ pcb_tool_name }}</PCB_TOOL_NAME>
			<!-- <PCB_TOOL_HT_SET>0.0</PCB_TOOL_HT_SET>
			<PCB_TOOL_HT_CHK>0.0</PCB_TOOL_HT_CHK> -->
			<PCB_TOOL_FEET_CHK>{{ pcb_tool_feet_chk_str }}</PCB_TOOL_FEET_CHK>
			<GLUE_TYPE>Araldite</GLUE_TYPE>
			<GLUE_BATCH_NUM>{{ adhesive_type }}</GLUE_BATCH_NUM>
			<MOD_GRADE>{{ grade }}</MOD_GRADE>
			<!-- <PCB_STEP>{{ step_pcb }}</PCB_STEP> -->
			<PCB_PLCMENT_X_OFFSET>{{ pcb_plcment_x_offset }}</PCB_PLCMENT_X_OFFSET>
			<PCB_PLCMENT_Y_OFFSET>{{ pcb_plcment_y_offset }}</PCB_PLCMENT_Y_OFFSET>
			<PCB_PLCMENT_ANG_OFFSET>{{ pcb_plcment_ang_offset }}</PCB_PLCMENT_ANG_OFFSET>
			<MOD_FLTNS_MM>{{ flatness }}</MOD_FLTNS_MM>
			<MOD_AVE_THKNS_MM>{{ thickness }}</MOD_AVE_THKNS_MM>
			<MOD_MAX_THKNS_MM>{{ max_thickness }}</MOD_MAX_THKNS_MM>
		</DATA>
	</DATA_SET>
</ROOT>
